read -p "enter number:" character

if((character == 1))
then
 echo "sunday"

elif((character == 2))
then
 echo "monday"

elif((character == 3))
then
 echo "tuesady"

elif((character == 4))
then
 echo "wednesday"

elif((character == 5))
then
 echo "turday"

elif((character == 6))
then
 echo "firday"

elif((character == 7))
then
 echo "saturday"

else
 echo "invalid day"

fi







